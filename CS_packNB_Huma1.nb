Notebook[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"RandomReal", "[", "]"}], "<", ".5"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"state", "=", 
           RowBox[{"RandomChoice", "[", 
            RowBox[{"CountryData", "[", "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"city", "=", 
           RowBox[{"First", "[", 
            RowBox[{"CountryData", "[", 
             RowBox[{"state", ",", "\"\<LargestCities\>\""}], "]"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"qStr", "=", 
           RowBox[{"\"\<the city of \>\"", "<>", 
            RowBox[{"EntityValue", "[", 
             RowBox[{"city", ",", "\"\<Name\>\""}], "]"}], "<>", 
            "\"\< in \>\"", "<>", 
            RowBox[{"EntityValue", "[", 
             RowBox[{"state", ",", "\"\<Name\>\""}], "]"}]}]}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"range", "=", 
           RowBox[{"Delete", "[", 
            RowBox[{
             RowBox[{"Range", "[", 
              RowBox[{
               RowBox[{"-", "24"}], ",", "24"}], "]"}], ",", "25"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"state", "=", 
           RowBox[{"RandomChoice", "[", 
            RowBox[{"AdministrativeDivisionData", "[", 
             TemplateBox[{"\"all US states\"",RowBox[{"EntityClass", "[", 
                 
                 RowBox[{
                  "\"AdministrativeDivision\"", ",", 
                   "\"USStatesAllStates\""}], "]"}],
               "\"EntityClass[\\\"AdministrativeDivision\\\", \
\\\"USStatesAllStates\\\"]\"","\"administrative divisions\""},
              "EntityClass"], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"city", "=", 
           RowBox[{"EntityValue", "[", 
            RowBox[{"state", ",", "\"\<CapitalCity\>\""}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"qStr", "=", 
           RowBox[{"StringReplace", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"EntityValue", "[", 
               RowBox[{"city", ",", "\"\<Name\>\""}], "]"}], "<>", 
              "\"\<, \>\"", "<>", 
              RowBox[{"EntityValue", "[", 
               RowBox[{"state", ",", "\"\<Name\>\""}], "]"}]}], ",", 
             RowBox[{"\"\<, United States\>\"", "\[Rule]", "\"\<\>\""}]}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"range", "=", 
           RowBox[{"Delete", "[", 
            RowBox[{
             RowBox[{"Range", "[", 
              RowBox[{
               RowBox[{"-", "6"}], ",", "6"}], "]"}], ",", "7"}], "]"}]}]}]}],
         "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{
        "\"\<What time is it in \>\"", "<>", "qStr", "<>", "\"\<?\>\""}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"LocalTime", "[", "city", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"choiceNos", "=", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"DatePlus", "[", 
             RowBox[{"ansNo", ",", 
              RowBox[{"{", 
               RowBox[{"#", ",", "\"\<Hour\>\""}], "}"}]}], "]"}], "&"}], "/@", 
           RowBox[{"RandomSample", "[", 
            RowBox[{"range", ",", "3"}], "]"}]}], ",", "ansNo"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"DateString", "[", 
           RowBox[{"#", ",", 
            RowBox[{"{", 
             RowBox[{
             "\"\<Hour12Short\>\"", ",", "\"\<:\>\"", ",", "\"\<Minute\>\"", 
              ",", "\"\< \>\"", ",", "\"\<AMPM\>\"", ",", "\"\< on \>\"", 
              ",", "\"\<DayName\>\""}], "}"}]}], "]"}], "&"}], "/@", 
         "choiceNos"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 

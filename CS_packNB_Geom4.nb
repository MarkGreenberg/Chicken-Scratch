Notebook[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"r", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "100"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"Style", "[", 
         RowBox[{
          RowBox[{"StringForm", "[", 
           RowBox[{
           "\"\<The radius of a circle measures `` units.\\nWhat is the \
measure of its circumference?\>\"", ",", "r"}], "]"}], ",", 
          RowBox[{"TextAlignment", "\[Rule]", "Center"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"2", " ", "\[Pi]", " ", "r"}], ",", ".001"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\"\<~\>\"", "<>", 
            RowBox[{"ToString", "[", "#", "]"}], "<>", "\"\< units\>\""}], 
           ")"}], "&"}], "/@", " ", 
         RowBox[{"Prepend", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"Round", "[", 
              RowBox[{"#", ",", ".001"}], "]"}], "&"}], "/@", " ", 
            RowBox[{"RandomReal", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"3", ",", 
                RowBox[{"ansNo", "*", "2"}]}], "}"}], ",", "3"}], "]"}]}], 
           ",", "ansNo"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Geom4\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"arcLen", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"6", ",", "200"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"units", "=", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"{", 
          RowBox[{
          "\"\<Miles\>\"", ",", "\"\<Millimeters\>\"", ",", "\"\<Meters\>\"", 
           ",", "\"\<Centimeters\>\"", ",", "\"\<Feet\>\"", ",", 
           "\"\<Inches\>\"", ",", "\"\<Yards\>\"", ",", 
           "\"\<Kilometers\>\""}], "}"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"alQuant", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{"arcLen", ",", "units"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"deg", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"10", ",", "350"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"\"\<A \>\"", "<>", 
         RowBox[{"ToString", "[", "deg", "]"}], "<>", 
         "\"\<\[Degree] arc of a circle has an arc length of \>\"", "<>", 
         RowBox[{"ToString", "[", "alQuant", "]"}], "<>", 
         "\"\<. What is the circle's radius?\>\""}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        FractionBox[
         RowBox[{"180", " ", "alQuant"}], 
         RowBox[{"deg", " ", "\[Pi]"}]]}], ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Quantity", "[", 

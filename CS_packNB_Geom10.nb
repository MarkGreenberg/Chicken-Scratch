Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"arcLen", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"6", ",", "200"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"units", "=", 
           RowBox[{"RandomChoice", "[", 
            RowBox[{"{", 
             RowBox[{
             "\"\<Miles\>\"", ",", "\"\<Millimeters\>\"", ",", 
              "\"\<Meters\>\"", ",", "\"\<Centimeters\>\"", ",", 
              "\"\<Feet\>\"", ",", "\"\<Inches\>\"", ",", "\"\<Yards\>\"", 
              ",", "\"\<Kilometers\>\""}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"alQuant", "=", 
           RowBox[{"Quantity", "[", 
            RowBox[{"arcLen", ",", "units"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"deg", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"10", ",", "350"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"\"\<A \>\"", "<>", 
            RowBox[{"ToString", "[", "deg", "]"}], "<>", 
            "\"\<\[Degree] arc of a circle has an arc length of \>\"", "<>", 
            RowBox[{"ToString", "[", "alQuant", "]"}], "<>", 
            "\"\<. What is the circle's radius?\>\""}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ansNo", "=", 
           FractionBox[
            RowBox[{"180", " ", "alQuant"}], 
            RowBox[{"deg", " ", "\[Pi]"}]]}], ";", "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"StringForm", "[", 
              RowBox[{"\"\<~`1`\>\"", ",", 
               RowBox[{"Round", "[", 
                RowBox[{"#", ",", ".01"}], "]"}]}], "]"}], "&"}], "/@", 
            RowBox[{"Prepend", "[", 
             RowBox[{
              RowBox[{"RandomVariate", "[", 
               RowBox[{
                RowBox[{"NormalDistribution", "[", 
                 RowBox[{"ansNo", ",", 
                  RowBox[{"ansNo", "/", "5"}]}], "]"}], ",", "3"}], "]"}], 
              ",", "ansNo"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{"choices", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}], ",", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         "2", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"pts2", "=", 
           RowBox[{"Partition", "[", 
            RowBox[{
             RowBox[{"Riffle", "[", 
              RowBox[{
               RowBox[{"RandomSample", "[", 
                RowBox[{
                 RowBox[{"Range", "[", 
                  RowBox[{
                   RowBox[{"-", "30"}], ",", "30"}], "]"}], ",", "2"}], "]"}],
                ",", 
               RowBox[{"RandomSample", "[", 
                RowBox[{
                 RowBox[{"Range", "[", 
                  RowBox[{
                   RowBox[{"-", "30"}], ",", "30"}], "]"}], ",", "2"}], "]"}]}
               ], "]"}], ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"rtVt", "=", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"RandomReal", "[", "]"}], "<", ".5"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"pts2", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
               RowBox[{"pts2", "\[LeftDoubleBracket]", 
                RowBox[{"2", ",", "2"}], "\[RightDoubleBracket]"}]}], "}"}], 
             ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"pts2", "\[LeftDoubleBracket]", 
                RowBox[{"2", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
               RowBox[{"pts2", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "2"}], "\[RightDoubleBracket]"}]}], "}"}]}],
             "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"pts", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{"Append", "[", 
             RowBox[{"pts2", ",", "rtVt"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"StringForm", "[", 
            RowBox[{
            "\"\<A right triangle has vertices at\\n(`1`, `2`), (`3`, `4`), \
and (`5`, `6`).\\nWhere is its circumcenter?\>\"", ",", 
             RowBox[{"pts", "\[LeftDoubleBracket]", 
              RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
             RowBox[{"pts", "\[LeftDoubleBracket]", 
              RowBox[{"1", ",", "2"}], "\[RightDoubleBracket]"}], ",", 
             RowBox[{"pts", "\[LeftDoubleBracket]", 
              RowBox[{"2", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
             RowBox[{"pts", "\[LeftDoubleBracket]", 
              RowBox[{"2", ",", "2"}], "\[RightDoubleBracket]"}], ",", 
             RowBox[{"pts", "\[LeftDoubleBracket]", 
              RowBox[{"3", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
             RowBox[{"pts", "\[LeftDoubleBracket]", 
              RowBox[{"3", ",", "2"}], "\[RightDoubleBracket]"}]}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"ansStr", "=", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Mean", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"pts2", "\[LeftDoubleBracket]", 
                 RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
                RowBox[{"pts2", "\[LeftDoubleBracket]", 
                 RowBox[{"2", ",", "1"}], "\[RightDoubleBracket]"}]}], "}"}], 
              "]"}], ",", 
             RowBox[{"Mean", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"pts2", "\[LeftDoubleBracket]", 
                 RowBox[{"1", ",", "2"}], "\[RightDoubleBracket]"}], ",", 
                RowBox[{"pts2", "\[LeftDoubleBracket]", 
                 RowBox[{"2", ",", "2"}], "\[RightDoubleBracket]"}]}], "}"}], 
              "]"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"dis", "=", 
           RowBox[{"Partition", "[", 
            RowBox[{
             RowBox[{"RandomSample", "[", 
              RowBox[{
               RowBox[{"Range", "[", 
                RowBox[{
                 RowBox[{"-", "30"}], ",", "30", ",", ".5"}], "]"}], ",", 
               "8"}], "]"}], ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
          
          RowBox[{"choices", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"StringReplace", "[", 
              RowBox[{
               RowBox[{"\"\<(\>\"", "<>", 
                RowBox[{"ToString", "[", 
                 RowBox[{
                 "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
                 "]"}], "<>", "\"\<, \>\"", "<>", 
                RowBox[{"ToString", "[", 
                 RowBox[{
                 "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
                 "]"}], "<>", "\"\<)\>\""}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"\"\<.,\>\"", "\[Rule]", "\"\<,\>\""}], ",", 
                 RowBox[{"\"\<.)\>\"", "\[Rule]", "\"\<)\>\""}]}], "}"}]}], 
              "]"}], "&"}], "/@", 
            RowBox[{"N", "[", 
             RowBox[{"Take", "[", 
              RowBox[{
               RowBox[{"DeleteDuplicates", "[", 
                RowBox[{"Prepend", "[", 
                 RowBox[{"dis", ",", "ansStr"}], "]"}], "]"}], ",", "4"}], 
              "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{"choices", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}]}], "]"}], 
       "&"}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
   "\"\<CS_pack_Geom10\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

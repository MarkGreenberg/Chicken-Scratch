Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ordinalizeF", "=", 
        RowBox[{
         RowBox[{"Module", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"mod", "=", 
             RowBox[{
              RowBox[{"Boole", "[", 
               RowBox[{"!", 
                RowBox[{"MemberQ", "[", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{"11", ",", "12"}], "}"}], ",", 
                  RowBox[{"Mod", "[", 
                   RowBox[{
                    RowBox[{"Abs", "[", "#", "]"}], ",", "100"}], "]"}]}], 
                 "]"}]}], "]"}], " ", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{"Abs", "[", "#", "]"}], ",", "10"}], "]"}]}]}], "}"}],
            ",", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"suffix", "=", 
               RowBox[{"Switch", "[", 
                RowBox[{
                "mod", ",", "1", ",", "\"\<st\>\"", ",", "2", ",", 
                 "\"\<nd\>\"", ",", "3", ",", "\"\<rd\>\"", ",", "_", ",", 
                 "\"\<th\>\""}], "]"}]}], "}"}], ",", 
             RowBox[{"Row", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"ToString", "@", "#"}], ",", "suffix"}], "}"}], 
              "]"}]}], "]"}]}], "]"}], "&"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"perc", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "99"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"prop", "=", 
        RowBox[{"RandomSample", "[", 
         RowBox[{
          RowBox[{"<|", 
           RowBox[{
            RowBox[{
            "\"\<BMI\>\"", "\[Rule]", "\"\<body mass index (BMI)\>\""}], ",", 
            
            RowBox[{"\"\<Height\>\"", "\[Rule]", "\"\<height\>\""}], ",", 
            RowBox[{"\"\<Weight\>\"", "\[Rule]", "\"\<weight\>\""}]}], "|>"}],
           ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"age", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"2", ",", "20"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"sex", "=", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Entity", "[", 
            RowBox[{"\"\<Gender\>\"", ",", "\"\<Male\>\""}], "]"}], ",", 
           RowBox[{"Entity", "[", 
            RowBox[{"\"\<Gender\>\"", ",", "\"\<Female\>\""}], "]"}]}], "}"}],
          "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"StringForm", "[", 
         RowBox[{
         "\"\<What is the `1` percentile `2` for a human `3` at age `4`?\>\"",
           ",", 
          RowBox[{"ordinalizeF", "[", "perc", "]"}], ",", 
          RowBox[{
           RowBox[{"Values", "[", "prop", "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", "sex", ",", "age"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"HumanGrowthData", "[", 
           RowBox[{
            RowBox[{"<|", 
             RowBox[{
              RowBox[{"\"\<Age\>\"", "\[Rule]", 
               RowBox[{"Quantity", "[", 
                RowBox[{"age", ",", "\"\<Years\>\""}], "]"}]}], ",", 
              RowBox[{"\"\<Gender\>\"", "\[Rule]", "sex"}]}], "|>"}], ",", 
            RowBox[{
             RowBox[{"Keys", "[", "prop", "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", 
            RowBox[{"N", "[", 
             RowBox[{"perc", "/", "100"}], "]"}]}], "]"}], ",", ".01"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ansSD", "=", 
        RowBox[{
         RowBox[{"QuantityMagnitude", "[", "ansNo", "]"}], "/", "4"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansUn", "=", 
        RowBox[{"QuantityUnit", "[", "ansNo", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"Quantity", "[", 
                RowBox[{"#", ",", "ansUn"}], "]"}], "&"}], "/@", 
              RowBox[{"Round", "[", 
               RowBox[{
                RowBox[{"RandomVariate", "[", 
                 RowBox[{
                  RowBox[{"NormalDistribution", "[", 
                   RowBox[{
                    RowBox[{"QuantityMagnitude", "[", "ansNo", "]"}], ",", 
                    "ansSD"}], "]"}], ",", "5"}], "]"}], ",", ".01"}], 
               "]"}]}], ",", "ansNo"}], "]"}], "]"}], ",", "4"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Anat12\>\"", ",", 
   
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

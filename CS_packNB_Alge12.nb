Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"no1", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "12"}], ",", "12"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"no2", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "12"}], ",", "12"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"no3", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "20"}], ",", "20"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{
         SuperscriptBox["no1", "2"], "-", 
         RowBox[{"no1", "*", "no2"}], "+", "no3"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{"RandomSample", "[", 
              RowBox[{
               RowBox[{"Range", "[", 
                RowBox[{
                 RowBox[{"-", "75"}], ",", "120"}], "]"}], ",", "4"}], "]"}], 
             ",", "ansNo"}], "]"}], "]"}], ",", "4"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 

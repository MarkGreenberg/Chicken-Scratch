Notebook[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"no1", "=", 
        RowBox[{
         RowBox[{"HoldForm", "[", 
          RowBox[{"\[Sqrt]", "rr"}], "]"}], "/.", 
         RowBox[{"rr", "\[Rule]", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "10000"}], "}"}], "]"}]}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"no2", "=", 
        RowBox[{
         RowBox[{"HoldForm", "[", 
          RadicalBox["rrr", "3",
           MultilineFunction->None,
           SurdForm->True], "]"}], "/.", 
         RowBox[{"rrr", "\[Rule]", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "1000000"}], "}"}], "]"}]}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"no3", "=", 
        RowBox[{
         RowBox[{"HoldForm", "[", 
          RowBox[{"bb", "^", "2"}], "]"}], "/.", 
         RowBox[{"bb", "\[Rule]", 
          RowBox[{"Round", "[", 
           RowBox[{
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"1", ",", "10"}], "}"}], "]"}], ",", ".001"}], 
           "]"}]}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"no4", "=", 
        RowBox[{
         RowBox[{"HoldForm", "[", 
          RowBox[{"bbb", "^", "3"}], "]"}], "/.", 
         RowBox[{"bbb", "\[Rule]", 
          RowBox[{"Round", "[", 
           RowBox[{
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"1", ",", "4.65"}], "}"}], "]"}], ",", ".001"}], 
           "]"}]}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"Hold", "[", "h1", "]"}], "/.", 
           RowBox[{"h1", "\[Rule]", "no1"}]}], ",", 
          RowBox[{
           RowBox[{"Hold", "[", "h2", "]"}], "/.", 
           RowBox[{"h2", "\[Rule]", "no2"}]}], ",", 
          RowBox[{
           RowBox[{"Hold", "[", "h3", "]"}], "/.", 
           RowBox[{"h3", "\[Rule]", "no3"}]}], ",", 
          RowBox[{
           RowBox[{"Hold", "[", "h4", "]"}], "/.", 
           RowBox[{"h4", "\[Rule]", "no4"}]}]}], "}"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"vals", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"N", "[", 
           RowBox[{"ReleaseHold", "[", "no1", "]"}], "]"}], ",", 
          RowBox[{"N", "[", 
           RowBox[{"ReleaseHold", "[", "no2", "]"}], "]"}], ",", 
          RowBox[{"ReleaseHold", "[", "no3", "]"}], ",", 
          RowBox[{"ReleaseHold", "[", "no4", "]"}]}], "}"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"vals", ",", 
           RowBox[{"Max", "[", "vals", "]"}]}], "]"}], "[", 

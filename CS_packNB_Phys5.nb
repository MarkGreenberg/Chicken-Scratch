Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"val\[Rho]", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"Round", "[", 
           RowBox[{"2000", "*", 
            RowBox[{"10", "^", 
             RowBox[{"RandomVariate", "[", 
              RowBox[{"NormalDistribution", "[", 
               RowBox[{"0", ",", ".45"}], "]"}], "]"}]}]}], "]"}], ",", 
          RowBox[{"\"\<Kilograms\>\"", "/", 
           RowBox[{"\"\<Meters\>\"", "^", "3"}]}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"valV", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"Round", "[", 
           RowBox[{
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{".01", ",", "2.5"}], "}"}], "]"}], ",", ".01"}], "]"}], 
          ",", 
          RowBox[{"\"\<Meters\>\"", "^", "3"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{
         RowBox[{"Hold", "[", 
          RowBox[{"Style", "[", 
           RowBox[{
            RowBox[{"StringForm", "[", 
             RowBox[{
             "\"\<If you already know that the density of an object is `1` \
and the volume is `2`, then what would its mass be?\>\"", ",", "h1", ",", 
              "h2"}], "]"}], ",", "28"}], "]"}], "]"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"h1", "\[Rule]", "val\[Rho]"}], ",", 
           RowBox[{"h2", "\[Rule]", "valV"}]}], "}"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansQnt", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"val\[Rho]", "*", "valV"}], ",", ".01"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansMag", "=", 
        RowBox[{"QuantityMagnitude", "[", "ansQnt", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Quantity", "[", 
             RowBox[{
              RowBox[{"Round", "[", 
               RowBox[{"#", ",", ".01"}], "]"}], ",", "\"\<Kilograms\>\""}], 
             "]"}], "&"}], "/@", 
           RowBox[{"RandomReal", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{".25", "*", "ansMag"}], ",", 
               RowBox[{"1.75", "*", "ansMag"}]}], "}"}], ",", "3"}], "]"}]}], 
          ",", "ansQnt"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", 
         RowBox[{"choices", ",", "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"pic", "=", 
        RowBox[{"Style", "[", 
         RowBox[{
          RowBox[{"Grid", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"\[Rho]", "\[Equal]", 
                 RowBox[{"M", "/", "V"}]}], ",", "SpanFromLeft"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"\[Rho]", ",", "\"\<mass\\ndensity\>\""}], "}"}], ",", 
              
              RowBox[{"{", 
               RowBox[{"M", ",", "\"\<mass\>\""}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"V", ",", "\"\<volume\>\""}], "}"}]}], "}"}], ",", 
            RowBox[{"Frame", "->", "All"}], ",", 
            RowBox[{"FrameStyle", "\[Rule]", 
             RowBox[{"Directive", "[", 
              RowBox[{"LightGray", ",", 
               RowBox[{"Thickness", "\[Rule]", "3"}]}], "]"}]}], ",", 
            RowBox[{"Alignment", "\[Rule]", "Left"}], ",", 
            RowBox[{"Spacings", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{"1.8", ",", "1.15"}], "}"}]}]}], "]"}], ",", "72"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], "]"}], 
        "&"}]}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 

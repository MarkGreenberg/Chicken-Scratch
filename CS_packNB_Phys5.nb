      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"val\[Rho]", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"Round", "[", 
           RowBox[{"2000", "*", 
            RowBox[{"10", "^", 
             RowBox[{"RandomVariate", "[", 
              RowBox[{"NormalDistribution", "[", 
               RowBox[{"0", ",", ".45"}], "]"}], "]"}]}]}], "]"}], ",", 
          RowBox[{"\"\<Kilograms\>\"", "/", 
           RowBox[{"\"\<Meters\>\"", "^", "3"}]}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"valV", "=", 
        RowBox[{"Quantity", "[", 

Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"scales", "=", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
            "\"\<\[Degree]C\>\"", ",", "\"\<Celsius\>\"", ",", 
             "\"\<DegreesCelsius\>\""}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
            "\"\<K\>\"", ",", "\"\<Kelvin\>\"", ",", "\"\<Kelvins\>\""}], 
            "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
            "\"\<\[Degree]R\>\"", ",", "\"\<Rankine\>\"", ",", 
             "\"\<DegreesRankine\>\""}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
            "\"\<\[Degree]R\[EAcute]\>\"", ",", "\"\<R\[EAcute]aumur\>\"", 
             ",", "\"\<DegreesReaumur\>\""}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
            "\"\<\[Degree]R\[OSlash]\>\"", ",", "\"\<R\[OSlash]mer\>\"", ",", 
             "\"\<DegreesRoemer\>\""}], "}"}]}], "}"}], "]"}]}], ";", 
       RowBox[{"degF", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "459"}], ",", "900"}], "}"}], "]"}], ",", 
          "\"\<DegreesFahrenheit\>\""}], "]"}]}], ";", 
       RowBox[{"deg1", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"UnitConvert", "[", 
           RowBox[{"degF", ",", 
            RowBox[{"scales", "[", 
             RowBox[{"[", "3", "]"}], "]"}]}], "]"}], ",", ".01"}], "]"}]}], 
       ";", 
       RowBox[{"q", "=", 
        RowBox[{"StringReplace", "[", 
         RowBox[{
          RowBox[{"\"\<The temperature \>\"", "<>", 
           RowBox[{"ToString", "[", "deg1", "]"}], "<>", 
           "\"\< is what in the Fahrenheit scale?\>\""}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"\"\< degrees\>\"", "\[Rule]", "\"\<\[Degree]\>\""}], ",", 
            RowBox[{"\"\<. \>\"", "\[Rule]", "\"\< \>\""}]}], "}"}]}], 
         "]"}]}], ";", 
       RowBox[{"choices", "=", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"ToString", "[", "#", "]"}], "<>", "\"\<\[Degree]F\>\""}], 
          "&"}], "/@", 
         RowBox[{"Prepend", "[", 
          RowBox[{
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "450"}], ",", "900"}], "}"}], ",", "3"}], "]"}], 
           ",", 
           RowBox[{"degF", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}]}], ";", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Phys1\>\"", ",", 

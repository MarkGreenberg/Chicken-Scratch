           RowBox[{"area1", ",", "\"\<Meters\>\""}], "]"}], ",", 
          RowBox[{"Style", "[", 
           RowBox[{"\"\<B\>\"", ",", "Italic"}], "]"}], ",", 
          RowBox[{"Quantity", "[", 
           RowBox[{"area2", ",", "\"\<Meters\>\""}], "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 

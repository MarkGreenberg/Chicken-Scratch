Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"area1", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"4", ",", "200"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"area2", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"4", ",", "200"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"Rationalize", "[", 
         RowBox[{
          FractionBox[
           RowBox[{"\[Sqrt]", 
            RowBox[{"(", 
             RowBox[{"area1", "/", "\[Pi]"}], ")"}]}], 
           RowBox[{"\[Sqrt]", 
            RowBox[{"(", 
             RowBox[{"area2", "/", "\[Pi]"}], ")"}]}]], ",", ".001"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"diss", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Rationalize", "[", 
           RowBox[{"#", ",", ".001"}], "]"}], "&"}], "/@", " ", 
         RowBox[{"Join", "[", 
          RowBox[{
           RowBox[{"RandomReal", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{".01", ",", "1"}], "}"}], ",", "5"}], "]"}], ",", 
           RowBox[{"RandomReal", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "7"}], "}"}], ",", "5"}], "]"}]}], "]"}]}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Prepend", "[", 
            RowBox[{"diss", ",", "ansNo"}], "]"}], "]"}], ",", "4"}], "]"}]}],
        ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 

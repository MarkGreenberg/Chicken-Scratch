Notebook[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"p1", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "16"}], "}"}], "]"}], ",", "\"\<Bars\>\""}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"v1", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"5", ",", "50"}], "}"}], "]"}], ",", 
          RowBox[{"\"\<Centimeters\>\"", "^", "3"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"p2", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "16"}], "}"}], "]"}], ",", "\"\<Bars\>\""}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"p1", "\[Equal]", "p2"}], ",", 
         RowBox[{"p2", "=", 
          RowBox[{"p2", "+", 
           RowBox[{"Quantity", "[", 
            RowBox[{"2", ",", "\"\<Bars\>\""}], "]"}]}]}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"v2", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"v1", "*", "p1"}], ")"}], "/", "p2"}], ",", ".01"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{
         RowBox[{"Hold", "[", 
          RowBox[{"StringForm", "[", 
           RowBox[{
           "\"\<According to Boyle's Law, if a gas has a volume of `1` at a \
pressure of `2`, then what is its volume at a pressure of `3`?\>\"", ",", 
            "h1", ",", "h2", ",", "h3"}], "]"}], "]"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"h1", "\[Rule]", "v1"}], ",", 
           RowBox[{"h2", "\[Rule]", "p1"}], ",", 
           RowBox[{"h3", "\[Rule]", "p2"}]}], "}"}]}]}], ";", 
       RowBox[{"choiceNos", "=", 
        RowBox[{"{", "v2", "}"}]}], ";", 
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "choiceNos", "]"}], "<", "4"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"choiceNos", "=", 
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Append", "[", 
            RowBox[{"choiceNos", ",", 
             RowBox[{"Quantity", "[", 
              RowBox[{
               RowBox[{"Round", "[", 
                RowBox[{
                 RowBox[{"RandomReal", "[", 
                  RowBox[{"{", 
                   RowBox[{"1", ",", 
                    RowBox[{
                    RowBox[{"QuantityMagnitude", "[", "v2", "]"}], "*", 
                    "1.8"}]}], "}"}], "]"}], ",", ".01"}], "]"}], ",", 
               RowBox[{"\"\<Centimeters\>\"", "^", "3"}]}], "]"}]}], "]"}], 
           "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", 
         RowBox[{"choiceNos", ",", "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choiceNos", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Phys3\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

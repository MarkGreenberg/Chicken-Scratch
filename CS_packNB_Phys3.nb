Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"pr", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{
             RowBox[{"Range", "[", 
              RowBox[{"1", ",", "30"}], "]"}], ",", "2"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"vo", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"2", ",", "20"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ansNo", "=", 
           RowBox[{"Round", "[", 
            RowBox[{
             FractionBox[
              RowBox[{"vo", "*", 
               RowBox[{
               "pr", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
              RowBox[{
              "pr", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]], 
             ",", ".1"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"StringForm", "[", 
            RowBox[{
            "\"\<According to Boyle's Law, if a gas has a volume of `1` at a \
pressure of `2` bars, then what is its volume at a pressure of `3` bars?\>\"",
              ",", 
             RowBox[{"Quantity", "[", 
              RowBox[{"vo", ",", 
               RowBox[{"\"\<Centimeters\>\"", "^", "3"}]}], "]"}], ",", 
             RowBox[{
             "pr", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
             ",", 
             RowBox[{
             "pr", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"StringForm", "[", 
              RowBox[{"\"\<`1` `2`\>\"", ",", 
               RowBox[{"StringReplace", "[", 
                RowBox[{
                 RowBox[{"ToString", "[", "#", "]"}], ",", 
                 RowBox[{
                  RowBox[{"\"\<.\>\"", "~~", "EndOfString"}], "\[Rule]", 
                  "\"\<\>\""}]}], "]"}], ",", 
               RowBox[{"cm", "^", "3"}]}], "]"}], "&"}], "/@", 
            RowBox[{"Take", "[", 
             RowBox[{
              RowBox[{"DeleteDuplicates", "[", 
               RowBox[{"Prepend", "[", 
                RowBox[{
                 RowBox[{"Abs", "[", 
                  RowBox[{"Round", "[", 
                   RowBox[{
                    RowBox[{"RandomVariate", "[", 
                    RowBox[{
                    RowBox[{"NormalDistribution", "[", 
                    RowBox[{"vo", ",", "25"}], "]"}], ",", "30"}], "]"}], ",",
                     ".1"}], "]"}], "]"}], ",", "ansNo"}], "]"}], "]"}], ",", 
              "4"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{
              "choices", "\[LeftDoubleBracket]", "1", 
               "\[RightDoubleBracket]"}]}], "]"}], "\[LeftDoubleBracket]", 
            RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}], ",", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         "2", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"speed", "=", 
           RowBox[{
            RowBox[{"Quantity", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"RandomInteger", "[", 
                RowBox[{"{", 
                 RowBox[{"20", ",", "400"}], "}"}], "]"}], "/", "10"}], ",", 
              RowBox[{"\"\<Meters\>\"", "/", "\"\<Seconds\>\""}]}], "]"}], "//",
             "N"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"radius", "=", 
           RowBox[{"Quantity", "[", 
            RowBox[{
             RowBox[{"RandomInteger", "[", 
              RowBox[{"{", 
               RowBox[{"5", ",", "300"}], "}"}], "]"}], ",", 
             "\"\<Meters\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"StringForm", "[", 
            RowBox[{
            "\"\<What is the centripetal acceleration of an object traveling \
at `1` around a `2` diameter circular path?\\n`3`\>\"", ",", "speed", ",", 
             RowBox[{"2", "*", "radius"}], ",", 
             RowBox[{"Style", "[", 
              RowBox[{
               RowBox[{"Hold", "[", 
                RowBox[{"HoldForm", "[", 
                 RowBox[{
                  SubscriptBox["a", "c"], "==", 
                  FractionBox[
                   SuperscriptBox["v", "2"], "r"]}], "]"}], "]"}], ",", 
               RowBox[{"Darker", "[", "Blue", "]"}]}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ansQua", "=", 
           RowBox[{"Round", "[", 
            RowBox[{
             FractionBox[
              SuperscriptBox["speed", "2"], "radius"], ",", ".01"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"disNo", "=", 
           RowBox[{"Abs", "[", 
            RowBox[{"Round", "[", 
             RowBox[{
              RowBox[{"RandomVariate", "[", 
               RowBox[{
                RowBox[{"NormalDistribution", "[", 
                 RowBox[{
                  RowBox[{"QuantityMagnitude", "[", "ansQua", "]"}], ",", 
                  RowBox[{".85", " ", 
                   RowBox[{"QuantityMagnitude", "[", "ansQua", "]"}]}]}], 
                 "]"}], ",", "4"}], "]"}], ",", ".01"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{"Take", "[", 
            RowBox[{
             RowBox[{"DeleteDuplicates", "[", 
              RowBox[{"Prepend", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"Quantity", "[", 
                   RowBox[{"#", ",", 
                    RowBox[{"\"\<Meters\>\"", "/", 
                    SuperscriptBox["\"\<Seconds\>\"", "2"]}]}], "]"}], "&"}], 
                 "/@", "disNo"}], ",", "ansQua"}], "]"}], "]"}], ",", "4"}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{
              "choices", "\[LeftDoubleBracket]", "1", 
               "\[RightDoubleBracket]"}]}], "]"}], "\[LeftDoubleBracket]", 
            RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}]}], "]"}], 
       "&"}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
   "\"\<CS_pack_Phys3\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

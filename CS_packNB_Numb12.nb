Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"nos", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"ToString", "[", "#", "]"}], "&"}], "/@", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "20"}], "}"}], ",", "5"}], "]"}]}], ",", 
          "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ops", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"RandomChoice", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
             "\"\< + \>\"", ",", "\"\< - \>\"", ",", "\"\< * \>\"", ",", 
              "\"\< / \>\""}], "}"}], ",", "4"}], "]"}], ",", "4"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", "}"}], ",", 
          RowBox[{"{", "}"}], ",", 
          RowBox[{"{", "}"}], ",", 
          RowBox[{"{", "}"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", 
         RowBox[{"i", "<", "5"}], ",", 
         RowBox[{"i", "++"}], ",", 
         RowBox[{
          RowBox[{"choices", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "=", 
          RowBox[{"StringJoin", "[", 
           RowBox[{"Riffle", "[", 
            RowBox[{
             RowBox[{"nos", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", 
             RowBox[{"ops", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "]"}]}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"qNo", "=", 
        RowBox[{"ToExpression", "[", 
         RowBox[{"choices", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"StringForm", "[", 
         RowBox[{
         "\"\<Which of these numeric expressions simplifies to the number \
`1`?\>\"", ",", "qNo"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Hold", "[", 
           RowBox[{"StringReplace", "[", 
            RowBox[{"#", ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"\"\</\>\"", "\[Rule]", 
                RowBox[{"FromCharacterCode", "[", "247", "]"}]}], ",", 
               RowBox[{"\"\<*\>\"", "\[Rule]", 
                RowBox[{"FromCharacterCode", "[", "8226", "]"}]}]}], "}"}]}], 
            "]"}], "]"}], "&"}], "/@", "mixed"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Numb12\>\"", ",", 
   

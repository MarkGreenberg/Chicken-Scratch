Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"man1", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "9.999"}], "}"}], "]"}], ",", ".01"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"exp1", "=", 
        RowBox[{
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"5", ",", "40"}], "}"}], "]"}], "*", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"RandomReal", "[", "]"}], "<", ".5"}], ",", 
           RowBox[{"-", "1"}], ",", "1"}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"man2", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "9.999"}], "}"}], "]"}], ",", ".01"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"exp2", "=", 
        RowBox[{
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"5", ",", "40"}], "}"}], "]"}], "*", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"RandomReal", "[", "]"}], "<", ".5"}], ",", 
           RowBox[{"-", "1"}], ",", "1"}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"no1", "=", 
        RowBox[{"ScientificForm", "[", 
         RowBox[{"man1", "*", 
          RowBox[{"10", "^", "exp1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       
       RowBox[{"no2", "=", 
        RowBox[{"ScientificForm", "[", 
         RowBox[{"man2", "*", 
          RowBox[{"10", "^", "exp2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       
       RowBox[{"manDiv", "=", 
        RowBox[{"(", 
         RowBox[{"man1", "/", "man2"}], ")"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ansExp", "=", 
        RowBox[{"exp1", "-", "exp2"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"manDiv", "<", "1"}], ",", 
         RowBox[{
          RowBox[{"manDiv", "*=", "10"}], ";", 
          RowBox[{"ansExp", "-=", "1"}]}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"manDiv", "=", 
        RowBox[{"Round", "[", 
         RowBox[{"manDiv", ",", ".01"}], "]"}]}], ";", "\[IndentingNewLine]", 
       
       RowBox[{"ansNo", "=", 
        RowBox[{"manDiv", "*", 
         RowBox[{"10", "^", "ansExp"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"disExps", "=", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{
           RowBox[{"ansExp", "-", "12"}], ",", 
           RowBox[{"ansExp", "+", "12"}]}], "]"}], "/.", 
         RowBox[{"ansExp", "\[Rule]", 
          RowBox[{"-", "ansExp"}]}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"disMans", "=", 
        RowBox[{"{", 
         RowBox[{"manDiv", ",", 
          RowBox[{"Round", "[", 
           RowBox[{
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"1", ",", "9.99"}], "}"}], "]"}], ",", ".01"}], "]"}], 
          ",", 
          RowBox[{"Round", "[", 
           RowBox[{
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"1", ",", "9.99"}], "}"}], "]"}], ",", ".01"}], "]"}]}],
          "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"disses", "=", 
        RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", 
         RowBox[{"i", "\[LessEqual]", 
          RowBox[{"Length", "[", "disExps", "]"}]}], ",", 
         RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"j", "=", "1"}], ",", 
           RowBox[{"j", "\[LessEqual]", 
            RowBox[{"Length", "[", "disMans", "]"}]}], ",", 
           RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{"disses", "=", 
            RowBox[{"Append", "[", 
             RowBox[{"disses", ",", 
              RowBox[{"ScientificForm", "[", 
               RowBox[{
                RowBox[{"disMans", "[", 
                 RowBox[{"[", "j", "]"}], "]"}], "*", 
                RowBox[{"10", "^", 
                 RowBox[{"disExps", "[", 
                  RowBox[{"[", "i", "]"}], "]"}]}]}], "]"}]}], "]"}]}]}], 
          "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{"RandomSample", "[", 
           RowBox[{"disses", ",", "3"}], "]"}], ",", "ansNo"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"StringForm", "[", 
         RowBox[{
         "\"\<Which of these is the approximate quotient of `1` divided by \
`2`?\>\"", ",", "no1", ",", "no2"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Numb15\>\"", ",", 
   

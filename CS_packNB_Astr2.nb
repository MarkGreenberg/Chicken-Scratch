              "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"disAlt", "=", 
           RowBox[{"Take", "[", 
            RowBox[{
             RowBox[{"DeleteDuplicates", "[", 
              RowBox[{"Round", "[", 

                    RowBox[{
                    RowBox[{"-", "90"}], ",", "90"}], "}"}], "]"}], ",", 
                  "6"}], "]"}], ",", ".01"}], "]"}], "]"}], ",", "3"}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"disAzm", "=", 
           RowBox[{"Take", "[", 
            RowBox[{
             RowBox[{"DeleteDuplicates", "[", 
              RowBox[{"Round", "[", 
               RowBox[{
                RowBox[{"Table", "[", 
                 RowBox[{
                  RowBox[{"RandomReal", "[", 
                   RowBox[{"{", 
                    RowBox[{"0", ",", "360"}], "}"}], "]"}], ",", "6"}], 
                 "]"}], ",", ".01"}], "]"}], "]"}], ",", "3"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"dis", "=", 
           RowBox[{"StringReplace", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"ToString", "[", "#", "]"}], "&"}], "/@", 
              RowBox[{"Partition", "[", 
               RowBox[{
                RowBox[{"Riffle", "[", 
                 RowBox[{"disAzm", ",", "disAlt"}], "]"}], ",", "2"}], 
               "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"\"\<{\>\"", "\[Rule]", "\"\<(\>\""}], ",", 
               RowBox[{"\"\<,\>\"", "\[Rule]", "\"\<\[Degree],\>\""}], ",", 
               RowBox[{"\"\<}\>\"", "\[Rule]", "\"\<\[Degree])\>\""}]}], 
              "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{"Prepend", "[", 
            RowBox[{"dis", ",", "sunPos"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{
          "q", "=", 
           "\"\<What is the position of the sun (azimuth, altitude) right now \
from your location?\>\""}], ";", "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 

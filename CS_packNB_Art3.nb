Notebook[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"q", "=", 
        RowBox[{"Style", "[", 
         RowBox[{
         "\"\<In digital art, colors are most often expressed as RGB \
hexidecimal numbers. Which of these represents the color shown here?\>\"", 
          ",", 
          RowBox[{"TextAlignment", "\[Rule]", "Center"}]}], "]"}]}], ";", 
       RowBox[{"choices", "=", 
        RowBox[{"StringTake", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"\"\<00000\>\"", "<>", 
             RowBox[{"StringDrop", "[", 
              RowBox[{
               RowBox[{"ToString", "[", 
                RowBox[{"BaseForm", "[", 
                 RowBox[{"#", ",", "16"}], "]"}], "]"}], ",", 
               RowBox[{"-", "9"}]}], "]"}]}], "&"}], "/@", 
           RowBox[{"RandomSample", "[", 
            RowBox[{
             RowBox[{"Range", "[", 
              RowBox[{"255", "^", "3"}], "]"}], ",", "4"}], "]"}]}], ",", 
          RowBox[{"-", "6"}]}], "]"}]}], ";", 
       RowBox[{"colors", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"RGBColor", "[", 
           RowBox[{"\"\<#\>\"", "<>", "#"}], "]"}], "&"}], "/@", 
         "choices"}]}], ";", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"pic", "=", 
        RowBox[{"Graphics", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"colors", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", 
            RowBox[{"Rectangle", "[", 
             RowBox[{"{", 
              RowBox[{"0", ",", "0"}], "}"}], "]"}]}], "}"}], ",", 
          RowBox[{"ImageSize", "\[Rule]", "480"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], "]"}], 
        "&"}]}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
   "\"\<CS_pack_Art3\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ordinalizeF", "=", 
        RowBox[{
         RowBox[{"Module", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"mod", "=", 
             RowBox[{
              RowBox[{"Boole", "[", 
               RowBox[{"!", 
                RowBox[{"MemberQ", "[", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{"11", ",", "12"}], "}"}], ",", 
                  RowBox[{"Mod", "[", 
                   RowBox[{
                    RowBox[{"Abs", "[", "#", "]"}], ",", "100"}], "]"}]}], 
                 "]"}]}], "]"}], " ", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{"Abs", "[", "#", "]"}], ",", "10"}], "]"}]}]}], "}"}],
            ",", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"suffix", "=", 
               RowBox[{"Switch", "[", 
                RowBox[{
                "mod", ",", "1", ",", "\"\<st\>\"", ",", "2", ",", 
                 "\"\<nd\>\"", ",", "3", ",", "\"\<rd\>\"", ",", "_", ",", 
                 "\"\<th\>\""}], "]"}]}], "}"}], ",", 
             RowBox[{"Row", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"ToString", "@", "#"}], ",", "suffix"}], "}"}], 
              "]"}]}], "]"}]}], "]"}], "&"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"perc", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "99"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"age", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "36"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"sex", "=", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Entity", "[", 
            RowBox[{"\"\<Gender\>\"", ",", "\"\<Male\>\""}], "]"}], ",", 
           RowBox[{"Entity", "[", 
            RowBox[{"\"\<Gender\>\"", ",", "\"\<Female\>\""}], "]"}]}], "}"}],
          "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"sexStr", "=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"sex", "===", 
           RowBox[{"Entity", "[", 
            RowBox[{"\"\<Gender\>\"", ",", "\"\<Male\>\""}], "]"}]}], ",", 
          "\"\<boy\>\"", ",", "\"\<girl\>\""}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"circ", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"QuantityMagnitude", "[", 
           RowBox[{"HumanGrowthData", "[", 
            RowBox[{
             RowBox[{"<|", 
              RowBox[{
               RowBox[{"\"\<Age\>\"", "\[Rule]", 
                RowBox[{"Quantity", "[", 
                 RowBox[{"age", ",", "\"\<Months\>\""}], "]"}]}], ",", 
               RowBox[{"\"\<Gender\>\"", "\[Rule]", "sex"}]}], "|>"}], ",", 
             "\"\<HeadCircumference\>\"", ",", 
             RowBox[{"N", "[", 
              RowBox[{"perc", "/", "100"}], "]"}]}], "]"}], "]"}], ",", 
          ".01"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"StringForm", "[", 
         RowBox[{
         "\"\<A `1`-month old baby `2` with a `3`-inch circumference head is \
in what percentile?\>\"", ",", "age", ",", "sexStr", ",", "circ"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Quantity", "[", 
           RowBox[{"#", ",", "\"\<Percent\>\""}], "]"}], "&"}], "/@", 
         RowBox[{"Take", "[", 
          RowBox[{
           RowBox[{"DeleteDuplicates", "[", 
            RowBox[{"Prepend", "[", 
             RowBox[{
              RowBox[{"RandomSample", "[", 
               RowBox[{
                RowBox[{"Range", "[", 
                 RowBox[{"1", ",", "99"}], "]"}], ",", "4"}], "]"}], ",", 
              "perc"}], "]"}], "]"}], ",", "4"}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Anat13\>\"", ",", 
   

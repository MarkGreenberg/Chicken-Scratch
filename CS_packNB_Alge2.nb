                    RowBox[{"StringForm", "[", 
                    RowBox[{"\"\<all real values of `1`\>\"", ",", 
                    RowBox[{"Style", "[", 
                    RowBox[{"\"\<x\>\"", ",", "Italic"}], "]"}]}], "]"}], 
                    ",", "\"\<none, {}\>\""}], "}"}]}], "]"}], "]"}], "]"}], 
                "]"}], ",", "3"}], "]"}], ",", 
             RowBox[{"ansExp", "[", 
              RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 

          RowBox[{"dis3", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"#", "/.", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"Less", "\[Rule]", "Greater"}], ",", 
                RowBox[{"Greater", "\[Rule]", "Less"}]}], "}"}]}], "&"}], "/@",
             "dis2"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"StringForm", "[", 
            RowBox[{
            "\"\<If `1`, for which values of `2` is `3` greater than \
`2`?\>\"", ",", 
             RowBox[{"y", "\[Equal]", 
              RowBox[{
               RowBox[{
                RowBox[{"coCon", " ", "[", 
                 RowBox[{"[", 

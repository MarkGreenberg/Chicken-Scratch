Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"roots", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "9"}], ",", "9"}], "}"}], ",", "7"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"nRoots", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{"roots", ",", 
             RowBox[{"RandomInteger", "[", 
              RowBox[{"{", 
               RowBox[{"1", ",", "4"}], "}"}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"dRoots", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{"roots", ",", 
             RowBox[{"RandomInteger", "[", 
              RowBox[{"{", 
               RowBox[{"1", ",", "4"}], "}"}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"nF", "=", 
           RowBox[{"PadRight", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"x", "-", "#"}], "&"}], "/@", "nRoots"}], ",", "4", 
             ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"dF", "=", 
           RowBox[{"PadRight", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"x", "-", "#"}], "&"}], "/@", "dRoots"}], ",", "4", 
             ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"ratExp", "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"nF", "[", 
                RowBox[{"[", "1", "]"}], "]"}], ")"}], 
              RowBox[{"(", 
               RowBox[{"nF", "[", 
                RowBox[{"[", "2", "]"}], "]"}], ")"}], 
              RowBox[{"(", 
               RowBox[{"nF", "[", 
                RowBox[{"[", "3", "]"}], "]"}], ")"}], 
              RowBox[{"(", 
               RowBox[{"nF", "[", 
                RowBox[{"[", "4", "]"}], "]"}], ")"}]}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"dF", "[", 
                RowBox[{"[", "1", "]"}], "]"}], ")"}], 
              RowBox[{"(", 
               RowBox[{"dF", "[", 
                RowBox[{"[", "2", "]"}], "]"}], ")"}], 
              RowBox[{"(", 
               RowBox[{"dF", "[", 
                RowBox[{"[", "3", "]"}], "]"}], ")"}], 
              RowBox[{"(", 
               RowBox[{"dF", "[", 
                RowBox[{"[", "4", "]"}], "]"}], ")"}]}], ")"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"IntegerQ", "[", 
             RowBox[{"Denominator", "[", "ratExp", "]"}], "]"}], ",", 
            RowBox[{"ratExp", "=", 
             RowBox[{"Divide", "[", 
              RowBox[{"ratExp", ",", 
               RowBox[{"x", "-", "10"}]}], "]"}]}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"dists", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"x", "-", "#"}], ")"}], "&"}], "/@", 
            RowBox[{"RandomSample", "[", 
             RowBox[{
              RowBox[{"Complement", "[", 
               RowBox[{
                RowBox[{"Range", "[", 
                 RowBox[{
                  RowBox[{"-", "9"}], ",", "9"}], "]"}], ",", "dRoots"}], 
               "]"}], ",", "3"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{"Join", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"First", "[", 
               RowBox[{"Last", "[", 
                RowBox[{"FactorList", "[", 
                 RowBox[{"Denominator", "[", "ratExp", "]"}], "]"}], "]"}], 
               "]"}], "}"}], ",", "dists"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"Style", "[", 
            RowBox[{
            "\"\<Which of these is a factor of the denominator of the \
rational function shown in this graph?\>\"", ",", 
             RowBox[{"TextAlignment", "\[Rule]", "Center"}]}], "]"}]}], ";", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{"choices", ",", "4"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{"choices", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"pic", "=", 
           RowBox[{
            RowBox[{"Hold", "[", 
             RowBox[{"Plot", "[", 
              RowBox[{"h1", ",", 
               RowBox[{"{", 
                RowBox[{"x", ",", 
                 RowBox[{"-", "12"}], ",", "12"}], "}"}], ",", 
               RowBox[{"Exclusions", "\[Rule]", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"Denominator", "[", "h1", "]"}], "\[Equal]", "0"}], 
                 "}"}]}], ",", 
               RowBox[{"PlotStyle", "\[Rule]", 
                RowBox[{"Directive", "[", 
                 RowBox[{"Thick", ",", "Blue"}], "]"}]}], ",", 
               "\[IndentingNewLine]", 
               RowBox[{"ExclusionsStyle", "\[Rule]", 
                RowBox[{"Directive", "[", 
                 RowBox[{"Dashed", ",", "Red"}], "]"}]}], ",", 
               RowBox[{"ImageSize", "\[Rule]", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"{", "460", "}"}], ",", 
                  RowBox[{"{", "540", "}"}]}], "}"}]}], ",", 
               RowBox[{"AxesStyle", "\[Rule]", "15"}], ",", 
               RowBox[{"AspectRatio", "\[Rule]", "1.24"}]}], "]"}], "]"}], "/.", 
            RowBox[{"h1", "\[Rule]", "ratExp"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], 
           "]"}]}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", "2", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"coCon", "=", 
           RowBox[{"Partition", "[", 
            RowBox[{
             RowBox[{"Riffle", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"RandomSample", "[", 
                 RowBox[{
                  RowBox[{"Range", "[", 
                   RowBox[{
                    RowBox[{"-", "25"}], ",", "25"}], "]"}], ",", "3"}], 
                 "]"}], "/.", " ", 
                RowBox[{"0", "\[Rule]", "2"}]}], ",", 
               RowBox[{"RandomSample", "[", 
                RowBox[{
                 RowBox[{"Range", "[", 
                  RowBox[{
                   RowBox[{"-", "99"}], ",", "99"}], "]"}], ",", "3"}], 
                "]"}]}], "]"}], ",", "2"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ansExp", "=", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"Solve", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"y", "\[Equal]", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], " ", "x"}], "+", 
                   RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}]}], "&&", 
                 RowBox[{"y", ">", "x"}]}], ",", "y"}], "]"}], "/.", 
              RowBox[{"{", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{
                 RowBox[{"List", "[", 
                  RowBox[{"List", "[", 
                   RowBox[{"Rule", "[", 
                    RowBox[{"_", ",", 
                    RowBox[{"ConditionalExpression", "[", 
                    RowBox[{"_", ",", 
                    RowBox[{"Element", "[", 
                    RowBox[{"x", ",", 
                    RowBox[{"Reals", "[", "]"}]}], "]"}]}], "]"}]}], "]"}], 
                   "]"}], "]"}], "\[Rule]", 
                 RowBox[{"StringForm", "[", 
                  RowBox[{"\"\<all real values of `1`\>\"", ",", 
                   RowBox[{"Style", "[", 
                    RowBox[{"\"\<x\>\"", ",", "Italic"}], "]"}]}], "]"}]}], 
                ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"List", "[", 
                  RowBox[{"List", "[", 
                   RowBox[{"Rule", "[", 
                    RowBox[{"_", ",", 
                    RowBox[{"ConditionalExpression", "[", 
                    RowBox[{"_", ",", "aa_"}], "]"}]}], "]"}], "]"}], "]"}], 
                 "\[Rule]", "aa"}], ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"List", "[", "]"}], "\[Rule]", 
                 "\"\<none, {}\>\""}]}], "}"}]}], "&"}], "/@", "coCon"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"dis1", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"#", "/.", 
              RowBox[{"{", 
               RowBox[{

Notebook[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"yr1", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"1950", ",", "2000"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"years", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"DateList", "[", 
           RowBox[{"ToString", "[", "#", "]"}], "]"}], "&"}], "/@", 
         RowBox[{"Range", "[", 
          RowBox[{"yr1", ",", 
           RowBox[{"yr1", "+", "20"}]}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"inc", "=", 
        RowBox[{
         RowBox[{"Sort", "[", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"100", ",", "20"}], "]"}], "]"}], "+", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"100", ",", "20"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"exp", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"200", ",", "20"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"Count", "[", 
         RowBox[{
          RowBox[{"inc", "-", "exp"}], ",", 
          RowBox[{"_", "?", "Positive"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"data1", "=", 
        RowBox[{"Partition", "[", 
         RowBox[{
          RowBox[{"Riffle", "[", 
           RowBox[{"years", ",", "inc"}], "]"}], ",", "2"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"data2", "=", 
        RowBox[{"Partition", "[", 
         RowBox[{
          RowBox[{"Riffle", "[", 
           RowBox[{"years", ",", "exp"}], "]"}], ",", "2"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"pic", "=", 
        RowBox[{"DateListStepPlot", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"data1", ",", "data2"}], "}"}], ",", 
          RowBox[{"PlotLabels", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Style", "[", 
              RowBox[{"\"\<income\>\"", ",", "20", ",", "Blue"}], "]"}], ",", 
             
             RowBox[{"Style", "[", 
              RowBox[{"\"\<expense\>\"", ",", "20", ",", "Red"}], "]"}]}], 
            "}"}]}], ",", 
          RowBox[{"PlotStyle", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"Blue", ",", "Thick"}], "}"}], ",", "Red"}], "}"}]}], 
          ",", 
          RowBox[{"AspectRatio", "\[Rule]", 
           FractionBox["11", "8"]}], ",", 
          RowBox[{"ImageSize", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", "480", "}"}], ",", 
             RowBox[{"{", "540", "}"}]}], "}"}]}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
       "q", "=", "\"\<How many years did income exceed expenses?\>\""}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{"RandomSample", "[", 
              RowBox[{
               RowBox[{"Range", "[", 
                RowBox[{"2", ",", "18"}], "]"}], ",", "4"}], "]"}], ",", 
             "ansNo"}], "]"}], "]"}], ",", "4"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], "]"}], 
        "&"}]}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
   "\"\<CS_pack_Grap10\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

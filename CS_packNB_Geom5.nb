          RowBox[{
           RowBox[{"ptPair", "[", "x_List", "]"}], ":=", 
           RowBox[{"DisplayForm", "[", 
            RowBox[{"StyleBox", "[", 
             RowBox[{
              RowBox[{"RowBox", "[", 
               RowBox[{"{", 
                RowBox[{"\"\<(\>\"", ",", 

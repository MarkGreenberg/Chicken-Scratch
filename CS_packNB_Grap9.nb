Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"tot", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"100", ",", "1000"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"cums", "=", 
        RowBox[{"Sort", "[", 
         RowBox[{"RandomSample", "[", 
          RowBox[{
           RowBox[{"Range", "[", 
            RowBox[{"1", ",", "tot"}], "]"}], ",", "3"}], "]"}], "]"}]}], ";",
        "\[IndentingNewLine]", 
       RowBox[{"quant", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"cums", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"cums", "[", 
            RowBox[{"[", "2", "]"}], "]"}], "-", 
           RowBox[{"cums", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"cums", "[", 
            RowBox[{"[", "3", "]"}], "]"}], "-", 
           RowBox[{"cums", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], ",", 
          RowBox[{"tot", "-", 
           RowBox[{"cums", "[", 
            RowBox[{"[", "3", "]"}], "]"}]}]}], "}"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"pic", "=", 
        RowBox[{"PieChart", "[", 
         RowBox[{"quant", ",", 
          RowBox[{"SectorOrigin", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"90", "\[Degree]"}], ",", 
             RowBox[{"-", "1"}]}], "}"}]}], ",", 
          RowBox[{"ChartLegends", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Style", "[", 
              RowBox[{"\"\<neither A nor B\>\"", ",", "18"}], "]"}], ",", 
             RowBox[{"Style", "[", 
              RowBox[{"\"\<only A\>\"", ",", "18"}], "]"}], ",", 
             RowBox[{"Style", "[", 
              RowBox[{"\"\<both A and B\>\"", ",", "18"}], "]"}], ",", 
             RowBox[{"Style", "[", 
              RowBox[{"\"\<only B\>\"", ",", "18"}], "]"}]}], "}"}]}]}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"qOp", "=", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\"\<against both measures\>\"", "\[Rule]", 
            RowBox[{"quant", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], ",", 
           RowBox[{"\"\<for A but against B\>\"", "\[Rule]", 
            RowBox[{"quant", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ",", 
           RowBox[{"\"\<for both measures\>\"", "\[Rule]", 
            RowBox[{"quant", "[", 
             RowBox[{"[", "3", "]"}], "]"}]}], ",", 
           RowBox[{"\"\<for B but against A\>\"", "\[Rule]", 
            RowBox[{"quant", "[", 
             RowBox[{"[", "4", "]"}], "]"}]}], ",", 
           RowBox[{"\"\<for A\>\"", "\[Rule]", 
            RowBox[{
             RowBox[{"quant", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "+", 
             RowBox[{"quant", "[", 
              RowBox[{"[", "3", "]"}], "]"}]}]}], ",", 
           RowBox[{"\"\<for B\>\"", "\[Rule]", 
            RowBox[{
             RowBox[{"quant", "[", 
              RowBox[{"[", "3", "]"}], "]"}], "+", 
             RowBox[{"quant", "[", 
              RowBox[{"[", "4", "]"}], "]"}]}]}], ",", 
           RowBox[{"\"\<for any measure\>\"", "\[Rule]", 
            RowBox[{
             RowBox[{"quant", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "+", 
             RowBox[{"quant", "[", 
              RowBox[{"[", "3", "]"}], "]"}], "+", 
             RowBox[{"quant", "[", 
              RowBox[{"[", "4", "]"}], "]"}]}]}], ",", 
           RowBox[{"\"\<for only one measure\>\"", "\[Rule]", 
            RowBox[{
             RowBox[{"quant", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "+", 
             RowBox[{"quant", "[", 
              RowBox[{"[", "4", "]"}], "]"}]}]}]}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"StringForm", "[", 
         RowBox[{
         "\"\<If `1` people voted on measures A and B, how many people voted \
`2`?\>\"", ",", "tot", ",", 
          RowBox[{"Keys", "[", "qOp", "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{"RandomSample", "[", 
              RowBox[{
               RowBox[{"Range", "[", 
                RowBox[{"1", ",", 
                 RowBox[{"tot", "-", "1"}]}], "]"}], ",", "4"}], "]"}], ",", 
             RowBox[{"Values", "[", "qOp", "]"}]}], "]"}], "]"}], ",", "4"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], "]"}], 
        "&"}]}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
   "\"\<CS_pack_Grap9\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

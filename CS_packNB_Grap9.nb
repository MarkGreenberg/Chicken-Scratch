Cell[CellGroupData[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"tot", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"100", ",", "1000"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"cums", "=", 
           RowBox[{"Sort", "[", 
            RowBox[{"RandomSample", "[", 
             RowBox[{
              RowBox[{"Range", "[", 
               RowBox[{"1", ",", "tot"}], "]"}], ",", "3"}], "]"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"quant", "=", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
             "cums", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
             ",", 
             RowBox[{
              RowBox[{
              "cums", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
              "-", 
              RowBox[{
              "cums", "\[LeftDoubleBracket]", "1", 
               "\[RightDoubleBracket]"}]}], ",", 
             RowBox[{
              RowBox[{
              "cums", "\[LeftDoubleBracket]", "3", "\[RightDoubleBracket]"}], 
              "-", 
              RowBox[{
              "cums", "\[LeftDoubleBracket]", "2", 
               "\[RightDoubleBracket]"}]}], ",", 
             RowBox[{"tot", "-", 
              RowBox[{
              "cums", "\[LeftDoubleBracket]", "3", 
               "\[RightDoubleBracket]"}]}]}], "}"}]}], ";", 
          "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{"pic", "=", 
           RowBox[{"PieChart", "[", 
            RowBox[{"quant", ",", 
             RowBox[{"SectorOrigin", "\[Rule]", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"90", "\[Degree]"}], ",", 
                RowBox[{"-", "1"}]}], "}"}]}], ",", 
             RowBox[{"ChartStyle", "\[Rule]", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"RGBColor", "[", "\"\<#E30B5D\>\"", "]"}], ",", 
                RowBox[{"RGBColor", "[", "\"\<#FFE135\>\"", "]"}], ",", 
                RowBox[{"RGBColor", "[", "\"\<#4F86F7\>\"", "]"}], ",", 
                RowBox[{"RGBColor", "[", "\"\<#F4AE72\>\"", "]"}]}], "}"}]}], 
             ",", 
             RowBox[{"ChartLegends", "\[Rule]", 
              RowBox[{"Placed", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"Style", "[", 
                   RowBox[{"\"\<neither A nor B\>\"", ",", "18"}], "]"}], ",", 
                  RowBox[{"Style", "[", 
                   RowBox[{"\"\<only A\>\"", ",", "18"}], "]"}], ",", 
                  RowBox[{"Style", "[", 
                   RowBox[{"\"\<both A and B\>\"", ",", "18"}], "]"}], ",", 
                  RowBox[{"Style", "[", 
                   RowBox[{"\"\<only B\>\"", ",", "18"}], "]"}]}], "}"}], ",",
                 "Below"}], "]"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", 
          RowBox[{"qOp", "=", 
           RowBox[{"RandomChoice", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"\"\<against both measures\>\"", "\[Rule]", 
               RowBox[{
               "quant", "\[LeftDoubleBracket]", "1", 
                "\[RightDoubleBracket]"}]}], ",", 
              RowBox[{"\"\<for A but against B\>\"", "\[Rule]", 
               RowBox[{
               "quant", "\[LeftDoubleBracket]", "2", 
                "\[RightDoubleBracket]"}]}], ",", 
              RowBox[{"\"\<for both measures\>\"", "\[Rule]", 
               RowBox[{
               "quant", "\[LeftDoubleBracket]", "3", 
                "\[RightDoubleBracket]"}]}], ",", 
              RowBox[{"\"\<for B but against A\>\"", "\[Rule]", 
               RowBox[{
               "quant", "\[LeftDoubleBracket]", "4", 
                "\[RightDoubleBracket]"}]}], ",", 
              RowBox[{"\"\<for A\>\"", "\[Rule]", 
               RowBox[{
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "2", 
                 "\[RightDoubleBracket]"}], "+", 
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "3", 
                 "\[RightDoubleBracket]"}]}]}], ",", 
              RowBox[{"\"\<for B\>\"", "\[Rule]", 
               RowBox[{
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "3", 
                 "\[RightDoubleBracket]"}], "+", 
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "4", 
                 "\[RightDoubleBracket]"}]}]}], ",", 
              RowBox[{"\"\<for any measure\>\"", "\[Rule]", 
               RowBox[{
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "2", 
                 "\[RightDoubleBracket]"}], "+", 
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "3", 
                 "\[RightDoubleBracket]"}], "+", 
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "4", 
                 "\[RightDoubleBracket]"}]}]}], ",", 
              RowBox[{"\"\<for only one measure\>\"", "\[Rule]", 
               RowBox[{
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "2", 
                 "\[RightDoubleBracket]"}], "+", 
                RowBox[{
                "quant", "\[LeftDoubleBracket]", "4", 
                 "\[RightDoubleBracket]"}]}]}]}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"StringForm", "[", 
            RowBox[{
            "\"\<If `1` people voted on measures A and B, how many people \
voted `2`?\>\"", ",", "tot", ",", 
             RowBox[{"Keys", "[", "qOp", "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{"Take", "[", 
            RowBox[{
             RowBox[{"DeleteDuplicates", "[", 
              RowBox[{"Prepend", "[", 
               RowBox[{
                RowBox[{"RandomSample", "[", 
                 RowBox[{
                  RowBox[{"Range", "[", 
                   RowBox[{"1", ",", 
                    RowBox[{"tot", "-", "1"}]}], "]"}], ",", "4"}], "]"}], 
                ",", 
                RowBox[{"Values", "[", "qOp", "]"}]}], "]"}], "]"}], ",", 
             "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{
              "choices", "\[LeftDoubleBracket]", "1", 
               "\[RightDoubleBracket]"}]}], "]"}], "\[LeftDoubleBracket]", 
            RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], 
           "]"}]}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", "2", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"rad", "=", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 
             RowBox[{".05", ",", ".95"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"fre", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{
             RowBox[{"Range", "[", 
              RowBox[{
               RowBox[{"-", "10"}], ",", "10", ",", ".01"}], "]"}], ",", 
             "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"pha", "=", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 
             RowBox[{"0", ",", 
              RowBox[{"2", " ", "\[Pi]"}]}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"StringReplace", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"ToString", "[", "#", "]"}], "<>", 
                "\"\< cycles\>\""}], ",", 
               RowBox[{"\"\<. \>\"", "\[Rule]", "\"\< \>\""}]}], "]"}], "&"}],
             "/@", "fre"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"pic", "=", 
           RowBox[{
            RowBox[{"ResourceFunction", "[", "\"\<EpicyclePlot\>\"", "]"}], 
            "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"1", ",", "1", ",", "0"}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"rad", ",", 
                 RowBox[{
                 "fre", "\[LeftDoubleBracket]", "1", 
                  "\[RightDoubleBracket]"}], ",", "pha"}], "}"}]}], "}"}], 
             ",", 
             RowBox[{"1.5", " ", "\[Pi]"}], ",", 
             RowBox[{"{", 
              RowBox[{"0", ",", 
               RowBox[{"1.5", " ", "\[Pi]"}]}], "}"}], ",", 
             RowBox[{"PathStyle", "\[Rule]", 
              RowBox[{"{", 
               RowBox[{"Red", ",", "Dashed", ",", 
                RowBox[{"Opacity", "[", ".3", "]"}]}], "}"}]}], ",", 
             RowBox[{"CircleStyle", "\[Rule]", "Blue"}], ",", 
             RowBox[{"ImageSize", "\[Rule]", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"{", "480", "}"}], ",", 
                RowBox[{"{", "540", "}"}]}], "}"}]}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{
          "q", "=", 
           "\"\<In this epicycle plot, what is the frequency of the secondary \
circle relative to the primary?\>\""}], ";", "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{
              "choices", "\[LeftDoubleBracket]", "1", 
               "\[RightDoubleBracket]"}]}], "]"}], "\[LeftDoubleBracket]", 
            RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], 
           "]"}]}]}], "]"}], "&"}]}], "]"}], "]"}], ",", 
   "\[IndentingNewLine]", "\"\<CS_pack_Grap9\>\"", ",", 
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

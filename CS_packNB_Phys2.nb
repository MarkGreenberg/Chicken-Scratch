Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"valP", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"15", ",", "50"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"valV1", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"3", ",", "12"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"valV2", "=", 
        RowBox[{"valV1", "+", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"3", ",", "16"}], "}"}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{
         RowBox[{"Hold", "[", 
          RowBox[{"Style", "[", 
           RowBox[{
            RowBox[{"StringForm", "[", 
             RowBox[{
             "\"\<This formula gives the power of a wind turbine. If `1` and \
then the wind picks up from `2` to `3`, what is the new power output of the \
turbine?\>\"", ",", "h1", ",", "h2", ",", "h3"}], "]"}], ",", "24"}], "]"}], 
          "]"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"h1", "\[Rule]", 
            RowBox[{"(", 
             RowBox[{"P", "==", 
              RowBox[{"Quantity", "[", 
               RowBox[{"valP", ",", "\"\<kW\>\""}], "]"}]}], ")"}]}], ",", 
           RowBox[{"h2", "\[Rule]", 
            RowBox[{"Quantity", "[", 
             RowBox[{"valV1", ",", "\"\<m/s\>\""}], "]"}]}], ",", 
           RowBox[{"h3", "\[Rule]", 
            RowBox[{"Quantity", "[", 
             RowBox[{"valV2", ",", "\"\<m/s\>\""}], "]"}]}]}], "}"}]}]}], ";",
        "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"valV2", "/", "valV1"}], ")"}], "^", "3"}], "*", "valP"}],
          "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"choiceNos", "=", 
        RowBox[{"{", "ansNo", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "choiceNos", "]"}], "<", "4"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"choiceNos", "=", 
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Append", "[", 
            RowBox[{"choiceNos", ",", 
             RowBox[{"Round", "[", 
              RowBox[{"RandomReal", "[", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"valP", "+", "10"}], ",", 
                 RowBox[{"ansNo", "*", "1.5"}]}], "}"}], "]"}], "]"}]}], 
            "]"}], "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"ToString", "[", 
           RowBox[{"Quantity", "[", 
            RowBox[{"#", ",", "\"\<kW\>\""}], "]"}], "]"}], "&"}], "/@", 
         "choiceNos"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", 
         RowBox[{"choices", ",", "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"pic", "=", 
        RowBox[{"Hold", "[", 
         RowBox[{"Style", "[", 
          RowBox[{
           RowBox[{"Grid", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"HoldForm", "[", 
                  RowBox[{"P", "\[Equal]", 
                   RowBox[{
                    RowBox[{"(", 
                    RowBox[{"1", "/", "2"}], ")"}], "*", "\[Mu]", "*", 
                    "\[Rho]", "*", "A", "*", 
                    RowBox[{"V", "^", "3"}]}]}], "]"}], ",", "SpanFromLeft"}],
                 "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"P", ",", "\"\<turbine power\>\""}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Rho]", ",", "\"\<air density\>\""}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"A", ",", "\"\<turbine area\>\""}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Mu]", ",", "\"\<turbine efficiency\>\""}], "}"}], 
               ",", 
               RowBox[{"{", 
                RowBox[{"V", ",", "\"\<wind velocity\>\""}], "}"}]}], "}"}], 
             ",", 
             RowBox[{"Frame", "\[Rule]", "All"}], ",", 
             RowBox[{"FrameStyle", "\[Rule]", 
              RowBox[{"Directive", "[", 
               RowBox[{"LightGray", ",", 
                RowBox[{"Thickness", "\[Rule]", "3"}]}], "]"}]}], ",", 
             RowBox[{"Alignment", "\[Rule]", "Left"}], ",", 
             RowBox[{"Spacings", "\[Rule]", 
              RowBox[{"{", 
               RowBox[{"1.8", ",", "1.15"}], "}"}]}]}], "]"}], ",", "72"}], 
          "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], "]"}], 
        "&"}]}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 

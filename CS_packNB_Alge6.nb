Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"mat", "=", 
           RowBox[{"Partition", "[", 
            RowBox[{
             RowBox[{"RandomChoice", "[", 
              RowBox[{
               RowBox[{"Range", "[", 
                RowBox[{
                 RowBox[{"-", "12"}], ",", "12"}], "]"}], ",", "4"}], "]"}], 
             ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"det", "=", 
           RowBox[{"Det", "[", "mat", "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"choices", " ", "=", 
           RowBox[{"{", "det", "}"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"While", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Length", "[", "choices", "]"}], "<", "4"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"try", "=", 
              RowBox[{"Round", "[", 
               RowBox[{"RandomVariate", "[", 
                RowBox[{"NormalDistribution", "[", 
                 RowBox[{"0", ",", "120"}], "]"}], "]"}], "]"}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"Not", "[", 
                RowBox[{"MemberQ", "[", 
                 RowBox[{"choices", ",", "try"}], "]"}], "]"}], ",", 
               RowBox[{"choices", "=", 
                RowBox[{"Append", "[", 
                 RowBox[{"choices", ",", "try"}], "]"}]}]}], "]"}]}]}], "]"}],
           ";", "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{
            RowBox[{"HoldComplete", "[", 
             RowBox[{"StringForm", "[", 
              RowBox[{
              "\"\<If `1` and `2`=|`3`|, then what is the value of `2`?\>\"", 
               ",", 
               RowBox[{"A", "\[Equal]", "h1"}], ",", "d", ",", "A"}], "]"}], 
             "]"}], "/.", 
            RowBox[{"{", 
             RowBox[{"h1", "\[Rule]", 
              RowBox[{"matrix", "[", "mat", "]"}]}], "}"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 

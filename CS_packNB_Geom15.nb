Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ht1", "=", 
        RowBox[{"UnitConvert", "[", 
         RowBox[{
          RowBox[{"Quantity", "[", 
           RowBox[{
            RowBox[{"RandomInteger", "[", 
             RowBox[{"{", 
              RowBox[{"53", ",", "76"}], "}"}], "]"}], ",", 
            "\"\<Inches\>\""}], "]"}], ",", 
          RowBox[{"MixedUnit", "[", 
           RowBox[{"{", 
            RowBox[{"\"\<Feet\>\"", ",", "\"\<Inches\>\""}], "}"}], "]"}]}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"sh1", "=", 
        RowBox[{"UnitConvert", "[", 
         RowBox[{
          RowBox[{"Quantity", "[", 
           RowBox[{
            RowBox[{"RandomInteger", "[", 
             RowBox[{"{", 
              RowBox[{"13", ",", "555"}], "}"}], "]"}], ",", 
            "\"\<Inches\>\""}], "]"}], ",", 
          RowBox[{"MixedUnit", "[", 
           RowBox[{"{", 
            RowBox[{"\"\<Feet\>\"", ",", "\"\<Inches\>\""}], "}"}], "]"}]}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ht2", "=", 
        RowBox[{"Quantity", "[", 
         RowBox[{
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"28", ",", "82"}], "}"}], "]"}], ",", "\"\<Feet\>\""}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"sh2", "=", 
        RowBox[{"Round", "[", 
         RowBox[{"UnitConvert", "[", 
          RowBox[{
           FractionBox[
            RowBox[{"sh1", "*", "ht2"}], "ht1"], ",", "\"\<Feet\>\""}], "]"}],
          "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{"StringForm", "[", 
         RowBox[{
         "\"\<Keala, `1` tall, casts a shadow `2` long. A nearby palm tree's \
shadow is `3` long. How tall is the tree?\>\"", ",", "ht1", ",", "sh1", ",", 
          "sh2"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"UnitConvert", "[", 
           RowBox[{
            FractionBox[
             RowBox[{"ht1", "*", "sh2"}], "sh1"], ",", "\"\<Feet\>\""}], 
           "]"}], ",", ".1"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"Quantity", "[", 
                RowBox[{"#", ",", "\"\<Feet\>\""}], "]"}], "&"}], "/@", 
              RowBox[{"Round", "[", 
               RowBox[{
                RowBox[{"RandomReal", "[", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{
                    RowBox[{"QuantityMagnitude", "[", "ansNo", "]"}], "*", 
                    ".4"}], ",", 
                    RowBox[{
                    RowBox[{"QuantityMagnitude", "[", "ansNo", "]"}], "*", 
                    "2"}]}], "}"}], ",", "6"}], "]"}], ",", ".1"}], "]"}]}], 
             ",", "ansNo"}], "]"}], "]"}], ",", "4"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}], "&"}]}]}], 
     "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<CS_pack_Geom15\>\"", ",", 
   
   RowBox[{"Permissions", "\[Rule]", "\"\<Public\>\""}]}], "]"}]], "Input",

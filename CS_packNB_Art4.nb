           "45", ",", "64", ",", "90", ",", "128"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"q", "=", 
        RowBox[{
         RowBox[{"Hold", "[", 
          RowBox[{"StringForm", "[", 
           RowBox[{
           "\"\<The `1`-number controls the brightness of a photograph. A \
`2`-mm lens set to `3` will open the aperture to what diameter?\>\"", ",", 
            RowBox[{"Style", "[", 
             RowBox[{"\"\<f\>\"", ",", "Italic"}], "]"}], ",", "h1", ",", 
            RowBox[{"HoldForm", "[", 
             RowBox[{"f", "/", "h2"}], "]"}]}], "]"}], "]"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"h1", "\[Rule]", "fl"}], ",", 
           RowBox[{"h2", "\[Rule]", "fn"}]}], "}"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"choices", "=", 
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"DeleteDuplicates", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"\"\<~\>\"", "<>", 
              RowBox[{"ToString", "[", 
               RowBox[{"Round", "[", 
                RowBox[{
                 RowBox[{"Abs", "[", "#", "]"}], ",", ".01"}], "]"}], "]"}], 

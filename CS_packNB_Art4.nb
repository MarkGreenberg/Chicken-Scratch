            RowBox[{"Prepend", "[", 
             RowBox[{
              RowBox[{"RandomVariate", "[", 
               RowBox[{
                RowBox[{"SkewNormalDistribution", "[", 
                 RowBox[{"22", ",", "75", ",", 
                  RowBox[{"-", ".5"}]}], "]"}], ",", "5"}], "]"}], ",", 
              RowBox[{"fl", "/", "fn"}]}], "]"}]}], "]"}], ",", "4"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"mixed", "=", 
        RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ans", "=", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"mixed", ",", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"InputForm", "[", 
         RowBox[{"{", 
          RowBox[{"q", ",", "ans", ",", "mixed", ",", "pic"}], "}"}], "]"}], 
        "&"}]}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 

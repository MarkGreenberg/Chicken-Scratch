Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"pic", "=", 
        RowBox[{"Style", "[", 
         RowBox[{
          RowBox[{"Grid", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"HoldForm", "[", 
                 RowBox[{"N", "\[Equal]", 
                  RowBox[{"f", "/", "D"}]}], "]"}], ",", "SpanFromLeft"}], 
               "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"N", ",", "\"\<f-number\>\""}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"f", ",", "\"\<focal length\>\""}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"D", ",", "\"\<diameter\\n of aperture\>\""}], "}"}]}],
              "}"}], ",", 
            RowBox[{"Frame", "->", "All"}], ",", 
            RowBox[{"FrameStyle", "\[Rule]", 
             RowBox[{"Directive", "[", 
              RowBox[{"LightGray", ",", 
               RowBox[{"Thickness", "\[Rule]", "4"}]}], "]"}]}], ",", 

Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"cors", "=", 
        RowBox[{"<|", 
         RowBox[{
          RowBox[{"2", "\[Rule]", "\"\<horizontal & vertical scales\>\""}], 
          ",", 
          RowBox[{"3", "\[Rule]", "\"\<horizontal scale & bubble size\>\""}], 
          ",", 
          RowBox[{"4", "\[Rule]", "\"\<horizontal scale & bubble color\>\""}],
           " ", ",", 
          RowBox[{"6", "\[Rule]", "\"\<vertical scale & bubble size\>\""}], 
          ",", 
          RowBox[{"8", "\[Rule]", "\"\<vertical scale & bubble color\>\""}], 
          ",", 
          RowBox[{"12", "\[Rule]", "\"\<bubble size & color\>\""}]}], 
         "|>"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"raw1", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "20"}], "}"}], "]"}], ",", "20"}], "]"}]}], ";",
        "\[IndentingNewLine]", 
       RowBox[{"raw2", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "20"}], "}"}], "]"}], ",", "20"}], "]"}]}], ";",
        "\[IndentingNewLine]", 
       RowBox[{"raw3", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "20"}], "}"}], "]"}], ",", "20"}], "]"}]}], ";",
        "\[IndentingNewLine]", 
       RowBox[{"raw4", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"RandomVariate", "[", 
           RowBox[{"NormalDistribution", "[", 
            RowBox[{"#", ",", "2.2"}], "]"}], "]"}], "&"}], "/@", "raw3"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"pre", "=", 
        RowBox[{"RandomSample", "[", 
         RowBox[{"{", 
          RowBox[{"raw1", ",", "raw2", ",", "raw3", ",", "raw4"}], "}"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"pos", "=", 
        RowBox[{"Flatten", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Position", "[", 
            RowBox[{"pre", ",", "raw3"}], "]"}], ",", 
           RowBox[{"Position", "[", 
            RowBox[{"pre", ",", "raw4"}], "]"}]}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansStr", "=", 
        RowBox[{"cors", "[", 
         RowBox[{
          RowBox[{"pos", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "*", 
          RowBox[{"pos", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"data", "=", 
        RowBox[{
         RowBox[{"Transpose", "[", "pre", "]"}], "/.", 

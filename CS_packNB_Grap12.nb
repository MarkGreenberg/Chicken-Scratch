          RowBox[{"{", 
           RowBox[{"a_", ",", "b_", ",", "c_", ",", "d_"}], "}"}], "\[Rule]", 
          
          RowBox[{"Rule", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"a", ",", "b", ",", "c"}], "}"}], ",", "d"}], 
           "]"}]}]}]}], ";", "\n", 
       RowBox[{
        RowBox[{"data", "[", 
         RowBox[{"[", 
          RowBox[{"All", ",", "2"}], "]"}], "]"}], "=", 
        RowBox[{"Rescale", "[", 
         RowBox[{"data", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "2"}], "]"}], "]"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"scale", "=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"RandomReal", "[", "]"}], "<", ".5"}], ",", 
          RowBox[{"{", 
           RowBox[{"1", ",", "20"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"20", ",", "1"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"data", "[", 
         RowBox[{"[", 
          RowBox[{"All", ",", "1", ",", "3"}], "]"}], "]"}], "=", 
        RowBox[{
         RowBox[{"Rescale", "[", 
          RowBox[{
           RowBox[{"data", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "1", ",", "3"}], "]"}], "]"}], ",", 
           "scale"}], "]"}], "*", "2.2"}]}], ";", "\n", 
       RowBox[{"grad", "=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"RandomReal", "[", "]"}], "<", ".5"}], ",", 
          RowBox[{"ColorData", "[", "\"\<SolarColors\>\"", "]"}], ",", 
          RowBox[{"ColorData", "[", 
           RowBox[{"{", 
            RowBox[{"\"\<SolarColors\>\"", ",", "\"\<Reverse\>\""}], "}"}], 
           "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ClearAll", "[", "bub", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"bub", "[", 

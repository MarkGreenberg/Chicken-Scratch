Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"sMult", "=", 
        RowBox[{"RandomReal", "[", 
         RowBox[{"{", 
          RowBox[{".5", ",", "5"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"sFun", "=", 
        RowBox[{"sMult", "*", "x"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"dMult", "=", 
        RowBox[{"RandomReal", "[", 
         RowBox[{"{", 
          RowBox[{".5", ",", "5"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"dCon", "=", 
        RowBox[{"RandomReal", "[", 
         RowBox[{"{", 
          RowBox[{"100", ",", "400"}], "}"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"dFun", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "dMult"}], "*", "x"}], "+", "dCon"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansNo", "=", 
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"y", "\[Equal]", "sFun"}], "&&", 
           RowBox[{"y", "\[Equal]", "dFun"}]}], ",", 
          RowBox[{"{", 

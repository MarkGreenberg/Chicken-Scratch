          "\[IndentingNewLine]", 
          RowBox[{"dFun", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"-", "dMult"}], "*", "x"}], "+", "dCon"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ansNo", "=", 
           RowBox[{"Solve", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"y", "\[Equal]", "sFun"}], "&&", 
              RowBox[{"y", "\[Equal]", "dFun"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}], ";", 

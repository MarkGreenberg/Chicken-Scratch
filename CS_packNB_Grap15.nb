Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"sMult", "=", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 
             RowBox[{".5", ",", "5"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"sFun", "=", 
           RowBox[{"sMult", "*", "x"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"dMult", "=", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 
             RowBox[{".5", ",", "5"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"dCon", "=", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 

          RowBox[{"DeleteCases", "[", 
           RowBox[{"allWks", ",", "week"}], "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ansStr", "=", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"FetalGrowthData", "[", 

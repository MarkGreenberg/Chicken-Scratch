Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ordinalizeF", "=", 
        RowBox[{
         RowBox[{"Module", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"mod", "=", 
             RowBox[{
              RowBox[{"Boole", "[", 
               RowBox[{"!", 
                RowBox[{"MemberQ", "[", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{"11", ",", "12"}], "}"}], ",", 
                  RowBox[{"Mod", "[", 
                   RowBox[{
                    RowBox[{"Abs", "[", "#", "]"}], ",", "100"}], "]"}]}], 
                 "]"}]}], "]"}], " ", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{"Abs", "[", "#", "]"}], ",", "10"}], "]"}]}]}], "}"}],
            ",", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"suffix", "=", 
               RowBox[{"Switch", "[", 
                RowBox[{
                "mod", ",", "1", ",", "\"\<st\>\"", ",", "2", ",", 
                 "\"\<nd\>\"", ",", "3", ",", "\"\<rd\>\"", ",", "_", ",", 
                 "\"\<th\>\""}], "]"}]}], "}"}], ",", 
             RowBox[{"Row", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"ToString", "@", "#"}], ",", "suffix"}], "}"}], 
              "]"}]}], "]"}]}], "]"}], "&"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"allWks", "=", 
        RowBox[{"{", 
         RowBox[{
         "1", ",", "2", ",", "4", ",", "5", ",", "6", ",", "7", ",", "8", ",",

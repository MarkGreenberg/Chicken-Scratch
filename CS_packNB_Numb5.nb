              RowBox[{"exp", ",", "4"}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}], ",", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         "2", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"pos", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"1", ",", "6"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"scen", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"1", ",", "3"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"Switch", "[", 
           RowBox[{
           "scen", ",", "\[IndentingNewLine]", "1", ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"int", "=", 
              RowBox[{"RandomInteger", "[", 
               RowBox[{"{", 
                RowBox[{"2", ",", "99"}], "}"}], "]"}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"qNo", "=", 
              RowBox[{"Sqrt", "[", "int", "]"}]}], ";", "\[IndentingNewLine]", 
             RowBox[{"qStr", "=", 
              RowBox[{"Sqrt", "[", "int", "]"}]}], ";", "\[IndentingNewLine]",

Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2", ",", "3"}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"exp", "=", 
           RowBox[{"RandomInteger", "[", "40", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"qNo", "=", 
           RowBox[{
            RowBox[{"HoldForm", "[", 
             RowBox[{"\[ImaginaryI]", "^", "n"}], "]"}], "/.", 
            RowBox[{"n", "\[Rule]", "exp"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{
            RowBox[{"Hold", "[", 
             RowBox[{"StringForm", "[", 
              RowBox[{
              "\"\<Simplify the imaginary number: ``\>\"", ",", "h1"}], "]"}],
              "]"}], "/.", 
            RowBox[{"h1", "\[Rule]", "qNo"}]}]}], ";", "\[IndentingNewLine]", 
          
          RowBox[{"mixed", "=", 
           RowBox[{"{", 
            RowBox[{"I", ",", 
             RowBox[{"-", "1"}], ",", 
             RowBox[{"-", "I"}], ",", "1"}], "}"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Mod", "[", 
               RowBox[{"exp", ",", "4"}], "]"}], "\[Equal]", "0"}], ",", "4", 

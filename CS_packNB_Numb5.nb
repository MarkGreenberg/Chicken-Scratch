          RowBox[{"q", "=", 
           RowBox[{"StringForm", "[", 
            RowBox[{
            "\"\<What is the `1` digit of the decimal form of `2`?\\n(Don't \
count leading zeros.)\>\"", ",", "ord", ",", "qStr"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"rd", "=", 
           RowBox[{"RealDigits", "[", 
            RowBox[{"qNo", ",", "10", ",", "6"}], "]"}]}], ";", 

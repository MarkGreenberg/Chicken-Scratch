Notebook[{
Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"exp", "=", 
           RowBox[{"RandomInteger", "[", "40", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"qNo", "=", 
           RowBox[{
            RowBox[{"HoldForm", "[", 
             RowBox[{"\[ImaginaryI]", "^", "n"}], "]"}], "/.", 
            RowBox[{"n", "\[Rule]", "exp"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{
            RowBox[{"Hold", "[", 
             RowBox[{"StringForm", "[", 
              RowBox[{
              "\"\<Simplify the imaginary number: ``\>\"", ",", "h1"}], "]"}],
              "]"}], "/.", 
            RowBox[{"h1", "\[Rule]", "qNo"}]}]}], ";", "\[IndentingNewLine]", 
          
          RowBox[{"mixed", "=", 
           RowBox[{"{", 
            RowBox[{"I", ",", 
             RowBox[{"-", "1"}], ",", 
             RowBox[{"-", "I"}], ",", "1"}], "}"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Mod", "[", 
               RowBox[{"exp", ",", "4"}], "]"}], "\[Equal]", "0"}], ",", "4", 
             ",", 
             RowBox[{"Mod", "[", 
              RowBox[{"exp", ",", "4"}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}], ",", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         "2", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"no", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"2", ",", "300"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"sqrt", "=", 
           RowBox[{"Sqrt", "[", "no", "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"pos", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"1", ",", "6"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ord", "=", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
             "\"\<first\>\"", ",", "\"\<second\>\"", ",", "\"\<third\>\"", 
              ",", "\"\<fourth\>\"", ",", "\"\<fifth\>\"", ",", 
              "\"\<sixth\>\""}], "}"}], "[", 
            RowBox[{"[", "pos", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"StringForm", "[", 
            RowBox[{
            "\"\<What is the `1` digit in the square root of the number \
`2`?\>\"", ",", "ord", ",", "no"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"ansNo", "=", 
           RowBox[{
            RowBox[{"RealDigits", "[", 
             RowBox[{"sqrt", ",", "10", ",", "8"}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "pos"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{"RandomSample", "[", 
              RowBox[{
               RowBox[{"DeleteCases", "[", 
                RowBox[{
                 RowBox[{"Range", "[", 
                  RowBox[{"0", ",", "9"}], "]"}], ",", "ansNo"}], "]"}], ",", 
               "3"}], "]"}], ",", "ansNo"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{"choices", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}]}], "]"}], 
       "&"}]}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
   "\"\<CS_pack_Numb5\>\"", ",", 

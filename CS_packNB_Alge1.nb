Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Delayed", "[", "\[IndentingNewLine]", 
    RowBox[{"APIFunction", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Switch", "[", 
        RowBox[{
         RowBox[{"RandomChoice", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2"}], "}"}], "]"}], ",", "\[IndentingNewLine]", 
         "1", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"choices", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{
             RowBox[{"Range", "[", 
              RowBox[{
               RowBox[{"-", "20"}], ",", "20"}], "]"}], ",", "4"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"xVal", "=", 
           RowBox[{"choices", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"co", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"2", ",", "12"}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"con", "=", 
           RowBox[{"Replace", "[", 
            RowBox[{
             RowBox[{"RandomInteger", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"-", "100"}], ",", "100"}], "}"}], "]"}], ",", 
             RowBox[{"0", "\[Rule]", "5"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"eqNo", "=", 
           RowBox[{
            RowBox[{"co", "*", "xVal"}], "+", "con"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"math", " ", "=", " ", 
           RowBox[{
            RowBox[{"Hold", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"h1", " ", "x"}], "+", "h2"}], "\[Equal]", "h3"}], 
             "]"}], "/.", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"h1", "\[Rule]", "co"}], ",", 
              RowBox[{"h2", "\[Rule]", "con"}], ",", 
              RowBox[{"h3", "\[Rule]", "eqNo"}]}], "}"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{"Style", "[", 
            RowBox[{
             RowBox[{"StringForm", "[", 
              RowBox[{
              "\"\<What is the value of `1` in the following \
equation?\\n`2`\>\"", ",", 
               RowBox[{"Style", "[", 
                RowBox[{"\"\<x\>\"", ",", "Italic"}], "]"}], ",", "math"}], 
              "]"}], ",", 
             RowBox[{"TextAlignment", "\[Rule]", "Center"}]}], "]"}]}], ";", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", 
            RowBox[{"choices", ",", "4"}], "]"}]}], ";", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{"choices", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}], ",", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         "2", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"co", "=", 
           RowBox[{
            RowBox[{"RandomInteger", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"-", "12"}], ",", "12"}], "}"}], ",", "6"}], "]"}], "/.",
             " ", 
            RowBox[{"0", "\[Rule]", "1"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"exp", "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"co", "[", 
                RowBox[{"[", "1", "]"}], "]"}], " ", "x"}], "+", 
              RowBox[{"co", "[", 
               RowBox[{"[", "2", "]"}], "]"}]}], ")"}], " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"co", "[", 
                RowBox[{"[", "3", "]"}], "]"}], " ", "x"}], "+", 
              RowBox[{"co", "[", 
               RowBox[{"[", "4", "]"}], "]"}]}], ")"}]}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"q", "=", 
           RowBox[{
            RowBox[{"Hold", "[", 
             RowBox[{"StringForm", "[", 
              RowBox[{
              "\"\<What are the {`1`, `2`, `3`} coefficients for the \
expansion of `4`?\>\"", ",", 
               RowBox[{"HoldForm", "[", 
                SuperscriptBox["x", "0"], "]"}], ",", 
               RowBox[{"HoldForm", "[", 
                SuperscriptBox["x", "1"], "]"}], ",", 
               SuperscriptBox["x", "2"], ",", "h1"}], "]"}], "]"}], "/.", 
            RowBox[{"h1", "\[Rule]", "exp"}]}]}], ";", "\[IndentingNewLine]", 
          
          RowBox[{"ansNos", "=", 
           RowBox[{"CoefficientList", "[", 
            RowBox[{
             RowBox[{"Expand", "[", 
              RowBox[{"ReleaseHold", "[", "exp", "]"}], "]"}], ",", "x"}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"choices", "=", 
           RowBox[{"Take", "[", 
            RowBox[{
             RowBox[{"DeleteDuplicates", "[", "\[IndentingNewLine]", 
              RowBox[{"Prepend", "[", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"RandomSample", "[", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"CoefficientList", "[", "\[IndentingNewLine]", 
                    RowBox[{
                    RowBox[{"Expand", "[", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], " ", "x"}], "+", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], ")"}], " ", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"#", "[", 
                    RowBox[{"[", "3", "]"}], "]"}], " ", "x"}], "+", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "4", "]"}], "]"}]}], ")"}]}], "]"}], ",", 
                    "\[IndentingNewLine]", "x"}], "]"}], "&"}], "/@", 
                  RowBox[{"Permutations", "[", 
                   RowBox[{"co", ",", 
                    RowBox[{"{", "4", "}"}]}], "]"}]}], "]"}], ",", 
                "ansNos"}], "]"}], "]"}], ",", "4"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"mixed", "=", 
           RowBox[{"RandomSample", "[", "choices", "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ans", "=", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"mixed", ",", 
              RowBox[{"choices", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"InputForm", "[", 
           RowBox[{"{", 
            RowBox[{"q", ",", "ans", ",", "mixed"}], "}"}], "]"}]}]}], "]"}], 
